{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["name", "desc"],
    "properties": {
      "name": { "type": "string" },
      "desc": { "type": "string" },
      "image": { "type": "string" },
      "icon": { "type": "string" },
      "date": { "type": "string" },
      "cost": { "type": ["string", "number"] }
    },
    "additionalProperties": true
  }
}
