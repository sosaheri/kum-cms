  "type": "array",
  "items": {
    "type": "object",
    "required": ["name", "desc"],
    "properties": {
      "name": { "type": "string" },
      "desc": { "type": "string" },
      "image": { "type": "string" },
      "clientLogo": { "type": "string" },
      "tags": { "type": "array", "items": { "type": "string" } }
    },
    "additionalProperties": true
  }
}
